<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#ffffe7a5" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <gradient android:startColor="#fff4cb76" android:endColor="#ffffd47b" android:angle="270.0" />
            <solid android:color="#ffffe7a5" />
        </shape>
    </item>
    <item>
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#ffbdefbd" />
        </shape>
    </item>
</selector>